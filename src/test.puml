@startuml

class Person {
  - name: String
  - age: int
  + getName(): String
  + getAge(): int
  + setName(name: String): void
  + setAge(age: int): void
}

class Student {
  - studentId: String
  + getStudentId(): String
  + setStudentId(studentId: String): void
}

Person "1" *-- "0..1" Student

@enduml